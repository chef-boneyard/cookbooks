############################################################################
#
#   This is an example config file for the autocomplete server
#
############################################################################

#Listening port configuration
port = 9191

#Dump file configuration - where we load/save the dictionary
file = /opt/doat/Search/data/autocomplete6.dump

#do we want to daemonize the server?
daemonize = true


#After how many changes do we want to save a dump? leave 0 to not save
#NOTE: save is blocking!!! @TODO: Fix this :)
maxSaveChanges = 0

#Do we want to precache all 2 letter prefixes on start?
precache = true
